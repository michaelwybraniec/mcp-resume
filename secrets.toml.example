# Example secrets file for Streamlit Cloud
# Copy this to .streamlit/secrets.toml and add your actual API keys

[llm_providers]
OPENROUTER_API_KEY = "your_openrouter_api_key_here"
OPENAI_API_KEY = "your_openai_api_key_here"
HUGGINGFACE_API_KEY = "your_huggingface_api_key_here"

[mcp_server]
# Path to your MCP resume server (if deployed together)
SERVER_PATH = "../build/index.js"

# Alternative: Use HTTP API endpoint if MCP server is deployed separately
# MCP_API_URL = "https://your-mcp-server.herokuapp.com" 